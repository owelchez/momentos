
<div class="container">
  <form class="box" action="/login" method="post">
    <h1>Login</h1>
    <label for="username">Username</label>
    <input  type="text"
            name="username"
            placeholder="Username"
    />
    <label for="password">Password</label>
    <input  type="password"
            name="password"
            placeholder="Password"
    />
    <input type="submit" value="Login">

  </form>
</div>
